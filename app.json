{
  "expo": {
    "name": "Ollama Mobile",
    "slug": "ollama-mobile",
    "version": "0.1.1",
    "orientation": "portrait",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "gradleVersion": "8.7",
            "androidGradlePluginVersion": "8.5.2"
          }
        }
      ],
      [
        "llama.rn",
        {
          "enableEntitlements": true,
          "entitlementsProfile": "production",
          "forceCxx20": true,
          "enableOpenCL": true
        }
      ],
      "./plugins/with-ollama-native",
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "fingerprint"
    },
    "platforms": ["android", "ios", "web"],
    "android": {
      "package": "com.ollamamobile.app",
      "versionCode": 2,
      "permissions": [],
      "usesCleartextTraffic": true
    },
    "extra": {
      "eas": {
        "projectId": "0a6f1c69-b232-40e3-9e6d-f3b2ed34980d"
      }
    }
  }
}
