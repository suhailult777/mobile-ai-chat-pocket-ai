#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/mobile-ai-chat - phase2/node_modules/.pnpm/expo@54.0.21_@babel+core@7._9e0a6abab35eafaced8493f1295b9754/node_modules/expo/bin/node_modules:/mnt/c/mobile-ai-chat - phase2/node_modules/.pnpm/expo@54.0.21_@babel+core@7._9e0a6abab35eafaced8493f1295b9754/node_modules/expo/node_modules:/mnt/c/mobile-ai-chat - phase2/node_modules/.pnpm/expo@54.0.21_@babel+core@7._9e0a6abab35eafaced8493f1295b9754/node_modules:/mnt/c/mobile-ai-chat - phase2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/mobile-ai-chat - phase2/node_modules/.pnpm/expo@54.0.21_@babel+core@7._9e0a6abab35eafaced8493f1295b9754/node_modules/expo/bin/node_modules:/mnt/c/mobile-ai-chat - phase2/node_modules/.pnpm/expo@54.0.21_@babel+core@7._9e0a6abab35eafaced8493f1295b9754/node_modules/expo/node_modules:/mnt/c/mobile-ai-chat - phase2/node_modules/.pnpm/expo@54.0.21_@babel+core@7._9e0a6abab35eafaced8493f1295b9754/node_modules:/mnt/c/mobile-ai-chat - phase2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../expo/bin/cli" "$@"
else
  exec node  "$basedir/../expo/bin/cli" "$@"
fi
