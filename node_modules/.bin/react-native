#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/mobile-ai-chat - phase2/node_modules/.pnpm/react-native@0.81.5_@babel+_90593d4ed3aa219ea593401cfa50ee72/node_modules/react-native/node_modules:/mnt/c/mobile-ai-chat - phase2/node_modules/.pnpm/react-native@0.81.5_@babel+_90593d4ed3aa219ea593401cfa50ee72/node_modules:/mnt/c/mobile-ai-chat - phase2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/mobile-ai-chat - phase2/node_modules/.pnpm/react-native@0.81.5_@babel+_90593d4ed3aa219ea593401cfa50ee72/node_modules/react-native/node_modules:/mnt/c/mobile-ai-chat - phase2/node_modules/.pnpm/react-native@0.81.5_@babel+_90593d4ed3aa219ea593401cfa50ee72/node_modules:/mnt/c/mobile-ai-chat - phase2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-native/cli.js" "$@"
else
  exec node  "$basedir/../react-native/cli.js" "$@"
fi
